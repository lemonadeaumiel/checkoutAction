{
  "name": "checkoutaction",
  "version": "1.0.0",
  "description": "Single endpoint used to performs checkout action on simplified e-commerce transaction. Done with node.js, express, sequalize, and postgreSQL.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "dev": "nodemon index",
    "test:db:seed": "npx sequelize-cli db:seed:all",
    "test:db:migrate" : "npx sequelize-cli db:migrate",
    "test:db:seed:undo": "npx sequelize-cli db:seed:undo",
    "test:db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "test:db:reset": "npx sequelize-cli db:migrate:undo",
    "test:db:reset:seed": "npm run test:db:reset&&npx sequelize-cli db:migrate&&npm run test:db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonadeaumiel/checkoutAction.git"
  },
  "author": "Florensia G",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lemonadeaumiel/checkoutAction/issues"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.1"
  },
  "homepage": "https://github.com/lemonadeaumiel/checkoutAction#readme",
  "keywords": [
    "async",
    "transaction",
    "nodejs",
    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "faker": "^6.6.6",
    "jest": "^29.2.0",
    "jest-cli": "^29.2.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.1",
    "supertest": "^6.3.0"
  },
  "db:create": "sequelize-cli db:create"
}
